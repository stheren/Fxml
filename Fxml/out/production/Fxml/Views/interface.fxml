<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>

<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.web.WebView?>

<?import javafx.scene.web.HTMLEditor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.text.Font?>
<AnchorPane id="Body" xmlns="http://javafx.com/javafx/8.0.201" xmlns:fx="http://javafx.com/fxml" fx:controller="Controllers.Main" stylesheets="@../Styles/style.css">
  <children>
    <MenuBar maxHeight="25.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem mnemonicParsing="false" text="New"/>
            <MenuItem mnemonicParsing="false" text="Save As"/>
            <MenuItem mnemonicParsing="false" text="Save"/>
            <SeparatorMenuItem/>
            <MenuItem mnemonicParsing="false" text="Open" onAction="#Open"/>
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Edit">
          <items>
            <MenuItem mnemonicParsing="false" text="Undo"/>
            <MenuItem mnemonicParsing="false" text="Cut"/>
            <MenuItem mnemonicParsing="false" text="Copy"/>
            <MenuItem mnemonicParsing="false" text="Path"/>
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About"/>
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane  prefHeight="160.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="25.0">
      <AnchorPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"  AnchorPane.topAnchor="0.0" maxWidth="200.0" minWidth="200.0">
        <ScrollPane fx:id="ListeElements" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="0.0"  AnchorPane.topAnchor="0.0" AnchorPane.rightAnchor="0.0">
        </ScrollPane>
        <AnchorPane id="ListeElements" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"  AnchorPane.rightAnchor="0.0" maxHeight="50.0" minHeight="50.0">
          <Button fx:id="CreateNewBtn" id="btnSuccess"  AnchorPane.bottomAnchor="2.0" AnchorPane.rightAnchor="2.0" text="+" onAction="#CreateNewFile"/>
        </AnchorPane>
      </AnchorPane>
        <AnchorPane id="Zone" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="200.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
          <HTMLEditor visible="false" fx:id="editor" prefHeight="373.0" prefWidth="370.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0" onMouseClicked="#textChange" onKeyPressed="#textChange"/>
          <WebView visible="false" fx:id="webView" prefHeight="373.0" prefWidth="370.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0"/>
        </AnchorPane>
    </AnchorPane>
  </children>
</AnchorPane>